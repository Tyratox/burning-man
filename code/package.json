{
  "name": "burning-man",
  "version": "1.0.0",
  "description": "Simulating the behaviour of people in the event of fire",
  "main": "src/index.ts",
  "scripts": {
    "build": "webpack --config webpack-prod.config.js ",
    "start": "webpack-dev-server --open",
    "prettier": "prettier --trailing-comma none --arrow-parens avoid --write ./src/**/*"
  },
  "config": {
    "ghooks": {
      "pre-commit": "cd code && npm run prettier && npm run build && cp style.css ../docs/style.css && cp ./assets/**/*.png ../docs/assets/ && cp ./assets/**/*.json ../docs/assets/ && touch ../.commit",
      "post-commit": "if [ -a .commit ]; then rm .commit && git add ./docs/* && git commit --amend -C HEAD --no-verify; fi"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tyratox/burning-man.git"
  },
  "authors": [
    "Nico Hauser",
    "Andri Horat",
    "Elias Schmid",
    "Jonas Spieler"
  ],
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/Tyratox/burning-man/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "file-loader": "^3.0.1",
    "ghooks": "^2.0.4",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "raw-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.6.0",
    "@types/lodash": "^4.14.144",
    "lodash": "^4.17.15",
    "phaser": "^3.16.2",
    "phaser-navmesh": "^2.0.5",
    "random-normal": "^1.0.0"
  }
}
